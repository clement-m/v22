<tr id="{{ data.taskForce }}{{ data.godName }}">
    <td class="player" data-playerId="{{ data.playerId }}">{% if data.playerName == null %}Player profile hidden{% else %}{{ data.playerName }}{% endif %}</td>
    <td class="god" data-godId="{{ data.godId }}"><img class="godImage" src="SRC/IMG/gods_icons/{{ data.godName }}.jpg" alt="{{ data.godName }}" /></td>
    <td class="account">{{ data.Account_Level }}</td>
    <td class="rank">{{ data.mastery_Level }}</td>
    <td class="godrank"><img class="loading" src="src/IMG/GIFLOAD/loading.gif" alt="loading" /></td>
    <td class="kda">{{ data.kda }}</td>
    <td class="conquest leagueWrapper"><img class="loading" src="SRC/IMG/GIFLOAD/loading.gif" alt="loading" /></td>
    <td class="joust leagueWrapper"><img class="loading" src="SRC/IMG/GIFLOAD/loading.gif" alt="loading" /></td>
    <td class="duel leagueWrapper"><img class="loading" src="SRC/IMG/GIFLOAD/loading.gif" alt="loading" /></td>
</tr>
