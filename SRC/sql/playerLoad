IN pi INT 11
IN pn VARCHAR 25
IN gi INT 5
IN gn VARCHAR 25
IN masteryLevel INT 3
IN taskForce INT 1
IN matchid INT 10

BEGIN
    IF((SELECT COUNT(*)
        FROM `player`
        WHERE `playerId`= pi ) > 0)
    THEN
        CALL updateMasteryLevel(pi,masteryLevel);
    ELSE
        INSERT INTO `player` VALUES (null, pi, pn, masteryLevel, 0);
        INSERT INTO `matchplayers` VALUES(null, pi, matchid, gi, taskForce);
    END IF;
END